{
  "name": "jsreport-templates",
  "version": "1.0.2",
  "description": "jsreport extension adding support for storing templates",
  "scripts": {
    "test": "mocha test --timeout 10000 && standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-templates",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-templates.git"
  },
  "standard": {
    "ignore": [
      "main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    },
    "parser": "babel-eslint"
  },
  "keywords": [
    "jsreport"
  ],
  "license": "LGPL",
  "dependencies": {
    "in-publish": "2.0.0",
    "node.extend.without.arrays": "1.1.5",
    "q": "1.4.1",
    "shortid": "2.2.6"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "6.0.4",
    "eslint": "2.12.0",
    "eslint-config-standard-jsx": "1.2.1",
    "eslint-plugin-babel": "3.2.0",
    "jsreport-core": "1.0.4",
    "jsreport-studio-dev": "1.0.0",
    "mocha": "2.5.3",
    "standard": "7.1.2"
  },
  "files": [
    "lib", "studio", "index.js", "jsreport.config.js"
  ]
}
