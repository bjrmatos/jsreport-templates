{
  "name": "jsreport-templates",
  "version": "0.1.0",
  "description": "jsreport extension adding support for storing templates",
  "scripts": {
    "test": "mocha test && standard",
    "prepublish": "node node_modules/jsreport-express/tools/optimize.js"
  },
  "homepage": "https://github.com/jsreport/jsreport-templates",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-templates.git"
  },
  "standard": {
    "ignore": [
      "public/**",
      "test/ui/**"
    ],
    "globals": [
      "describe",
      "it",
      "beforeEach"
    ]
  },
  "keywords": [
    "jsreport"
  ],
  "license": "LGPL",
  "dependencies": {
    "node.extend.without.arrays": "1.1.5",
    "q": "1.4.1",
    "shortid": "2.2.4",
    "stream": "0.0.2",
    "string": "3.3.1",
    "underscore": "1.8.3",
    "uuid": "2.0.1"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "eslint": "^1.9.0",
    "jsreport-core": "*",
    "jsreport-express": "*",
    "mocha": "^2.3.3",
    "standard": "^5.3.1"
  }
}
